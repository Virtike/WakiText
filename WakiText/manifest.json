{
  "manifest_version": 2,
 
  "name": "WakiText",
  "version": "0.1.0",
  "description": "Change Waki comments on PinkBike into a more appropriate style",

  "web_accessible_resources": [ "/*" ],

  "background": [{
        "scripts": ["thirdParty/jquery.min.js"]
  }],
 
  "content_scripts": [{
    "js": ["thirdParty/jquery.min.js", "content.js"],
    "matches": ["https://www.pinkbike.com/*"]
  }]
 
}